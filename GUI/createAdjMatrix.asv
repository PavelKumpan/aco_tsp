function createAdjMatrix()
% vytvoøí matici sousednosti ze zadaných bodù

global inputPoints;
global adjMatrix;
adjMatrix = [];

[isize i]   = size(inputPoints);

for(j = 1:isize)
    adjMatrix
   
    for(i = 1:isize)
        distance = sqrt(sum((inputPoints(j, :) - inputPoints(i, :)).^2));
        adjMatrix(j, i) = distance;
        adjMatrix(i, j) = distance;
    end;
end;

end

